From 05a281c3f3989a4dfe7de1bdc4ae7130f28eda4d Mon Sep 17 00:00:00 2001
From: sachnr <sachinrawat544@gmail.com>
Date: Thu, 19 Jan 2023 16:16:27 +0530
Subject: [PATCH] titlebar

---
 themes/Gruvbox-Light-B/gtk-3.0/gtk.css | 64 +++++++++++++-------------
 1 file changed, 32 insertions(+), 32 deletions(-)

diff --git a/themes/Gruvbox-Light-B/gtk-3.0/gtk.css b/themes/Gruvbox-Light-B/gtk-3.0/gtk.css
index 1a68e54b..8608fd43 100644
--- a/themes/Gruvbox-Light-B/gtk-3.0/gtk.css
+++ b/themes/Gruvbox-Light-B/gtk-3.0/gtk.css
@@ -2933,22 +2933,22 @@ headerbar
 .titlebar {
   transition: background-color 75ms cubic-bezier(0, 0, 0.2, 1),
     color 75ms cubic-bezier(0, 0, 0.2, 1);
-  background-color: #32302f;
-  color: #f9f5d7;
+  background-color: #f2e5bc;
+  color: #32302f;
   border-radius: 11px 11px 0 0;
 }
 
 .titlebar:disabled {
-  color: rgba(249, 245, 215, 0.5);
+  color: rgba(50, 48, 47, 0.5);
 }
 
 .titlebar:backdrop {
-  background-color: #32302f;
-  color: rgba(249, 245, 215, 0.7);
+  background-color: #f2e5bc;
+  color: rgba(50, 48, 47, 0.7);
 }
 
 .titlebar:backdrop:disabled {
-  color: rgba(249, 245, 215, 0.3);
+  color: rgba(50, 48, 47, 0.3)
 }
 
 .titlebar .title {
@@ -2964,12 +2964,12 @@ headerbar
 .titlebar .subtitle,
 .titlebar .dim-label {
   transition: color 75ms cubic-bezier(0, 0, 0.2, 1);
-  color: rgba(249, 245, 215, 0.7);
+  color: rgba(50, 48, 47, 0.7);
 }
 
 .titlebar .subtitle:backdrop,
 .titlebar .dim-label:backdrop {
-  color: rgba(249, 245, 215, 0.5);
+  color: rgba(50, 48, 47, 0.5);
 }
 
 .titlebar .titlebar {
@@ -2978,7 +2978,7 @@ headerbar
 }
 
 .titlebar headerbar + separator {
-  background-color: rgba(249, 245, 215, 0.12);
+  background-color: rgba(50, 48, 47, 0.12);
 }
 
 .titlebar .linked:not(.vertical) > entry {
@@ -2989,21 +2989,21 @@ headerbar
 
 .titlebar button.suggested-action:disabled,
 .titlebar button.destructive-action:disabled {
-  background-color: rgba(249, 245, 215, 0.04);
-  color: rgba(249, 245, 215, 0.5);
+  background-color: rgba(50, 48, 47, 0.04);
+  color: rgba(50, 48, 47, 0.5);
 }
 
 .titlebar stackswitcher {
-  border: 2px solid #f5edca;
+  border: 2px solid #32302f;
 }
 
 .titlebar
   stackswitcher.linked:not(.vertical)
   > button.text-button:not(.suggested-action):not(.destructive-action):active {
-  box-shadow: inset 0 0 0 9999px rgba(249, 245, 215, 0.08);
+  box-shadow: inset 0 0 0 9999px rgba(50, 48, 47, 0.08);
   background-image: radial-gradient(
     circle,
-    rgba(249, 245, 215, 0.08) 10%,
+    rgba(50, 48, 47, 0.08) 10%,
     transparent 0%
   );
 }
@@ -3011,8 +3011,8 @@ headerbar
 .titlebar
   stackswitcher.linked:not(.vertical)
   > button.text-button:not(.suggested-action):not(.destructive-action):checked {
-  background-color: #f5edca;
-  color: #32302f;
+  background-color: #32302f;
+  color: #f5edca;
 }
 
 .titlebar
@@ -3027,17 +3027,17 @@ headerbar
   transition: background-color 0.1ms 225ms,
     color 75ms cubic-bezier(0, 0, 0.2, 1);
   animation: ripple-on-headerbar 225ms cubic-bezier(0, 0, 0.2, 1);
-  box-shadow: inset 0 1px rgba(249, 245, 215, 0.1);
-  background-color: #3c3836;
-  color: #f9f5d7;
+  box-shadow: inset 0 1px rgba(50, 48, 47, 0.1);
+  background-color: #f9f5d7;
+  color: #3c3836;
 }
 
 .titlebar.selection-mode:backdrop {
-  color: rgba(249, 245, 215, 0.7);
+  color: rgba(50, 48, 47, 0.7);
 }
 
 .titlebar.selection-mode .subtitle:link {
-  color: #f9f5d7;
+  color: #3c3836;
 }
 
 .titlebar.selection-mode
@@ -3048,12 +3048,12 @@ headerbar
       transparent 0%
     )
     0 0 0/0 0 0px;
-  color: #f9f5d7;
+  color: #3c3836;
 }
 
 .titlebar.selection-mode
   button:not(.suggested-action):not(.destructive-action):disabled {
-  color: rgba(249, 245, 215, 0.5);
+  color: rgba(50, 48, 47, 0.5);
 }
 
 .titlebar.selection-mode
@@ -3064,32 +3064,32 @@ headerbar
       transparent 0%
     )
     0 0 2/0 0 2px;
-  color: #f9f5d7;
+  color: #3c3836;
 }
 
 .titlebar.selection-mode
   button:not(.suggested-action):not(.destructive-action):checked:disabled {
-  color: rgba(249, 245, 215, 0.5);
+  color: rgba(50, 48, 47, 0.5);
 }
 
 .titlebar.selection-mode
   button:not(.suggested-action):not(.destructive-action):backdrop {
-  color: rgba(249, 245, 215, 0.7);
+  color: rgba(50, 48, 47, 0.7);
 }
 
 .titlebar.selection-mode
   button:not(.suggested-action):not(.destructive-action):backdrop:disabled {
-  color: rgba(249, 245, 215, 0.3);
+  color: rgba(50, 48, 47, 0.3)
 }
 
 .titlebar.selection-mode
   button:not(.suggested-action):not(.destructive-action):backdrop:checked {
-  color: rgba(249, 245, 215, 0.7);
+  color: rgba(50, 48, 47, 0.7);
 }
 
 .titlebar.selection-mode
   button:not(.suggested-action):not(.destructive-action):backdrop:checked:disabled {
-  color: rgba(249, 245, 215, 0.3);
+  color: rgba(50, 48, 47, 0.3)
 }
 
 .titlebar.selection-mode .selection-menu {
@@ -3106,8 +3106,8 @@ headerbar
 }
 
 .titlebar .selection-mode {
-  box-shadow: inset 0 1px rgba(249, 245, 215, 0.1);
-  background-color: #3c3836;
+  box-shadow: inset 0 1px rgba(50, 48, 47, 0.1);
+  background-color: #f9f5d7;
 }
 
 .tiled .titlebar,
@@ -3160,7 +3160,7 @@ headerbar:not(.titlebar) {
 }
 
 box.vertical headerbar {
-  background-color: #32302f;
+  background-color: #f9f5d7;
 }
 
 headerbar > box.left,
-- 
